<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="controller.RegistrarActivity">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="@dimen/alturaAsterisco"
        android:layout_height="@dimen/anchuraAsterisco"
        android:layout_marginTop="@dimen/dim40"
        android:gravity="center"
        android:text="@string/fieldObligatorio"
        app:layout_constraintEnd_toStartOf="@+id/txtEmailRegistrar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="@dimen/alturaAsterisco"
        android:layout_height="@dimen/anchuraAsterisco"
        android:layout_marginTop="@dimen/dim40"
        android:gravity="center"
        android:text="@string/fieldObligatorio"
        app:layout_constraintEnd_toStartOf="@+id/txtEmailRegistrar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="@dimen/alturaAsterisco"
        android:layout_height="@dimen/anchuraAsterisco"
        android:layout_marginTop="@dimen/dim40"
        android:gravity="center"
        android:text="@string/fieldObligatorio"
        app:layout_constraintEnd_toStartOf="@+id/txtEmailRegistrar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="@dimen/alturaAsterisco"
        android:layout_height="@dimen/anchuraAsterisco"
        android:layout_marginTop="@dimen/dim40"
        android:gravity="center"
        android:text="@string/fieldObligatorio"
        app:layout_constraintEnd_toStartOf="@+id/txtEmailRegistrar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <EditText
        android:id="@+id/txtApellido2Registrar"
        android:layout_width="@dimen/anchuraTxt"
        android:layout_height="@dimen/alturaTxt"
        android:layout_marginTop="@dimen/dim20"
        android:autofillHints="@string/hintApellido_2"
        android:backgroundTint="@color/ColorPrincipal"
        android:ems="10"
        android:hint="@string/hintApellido_2"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtApellido1Registrar" />

    <EditText
        android:id="@+id/txtApellido1Registrar"
        android:layout_width="@dimen/anchuraTxt"
        android:layout_height="@dimen/alturaTxt"
        android:layout_marginTop="@dimen/dim20"
        android:autofillHints=""
        android:backgroundTint="@color/ColorPrincipal"
        android:ems="10"
        android:hint="@string/hintApellido_1"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtNombre" />

    <EditText
        android:id="@+id/txtNombre"
        android:layout_width="@dimen/anchuraTxt"
        android:layout_height="@dimen/alturaTxt"
        android:layout_marginTop="@dimen/dim20"
        android:autofillHints="@string/hintNombre"
        android:backgroundTint="@color/ColorPrincipal"
        android:ems="10"
        android:hint="@string/hintNombre"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtNickRegistrar" />

    <EditText
        android:id="@+id/txtNickRegistrar"
        android:layout_width="@dimen/anchuraTxt"
        android:layout_height="@dimen/alturaTxt"
        android:layout_marginTop="@dimen/dim20"
        android:autofillHints="@string/hintNick"
        android:backgroundTint="@color/ColorPrincipal"
        android:ems="10"
        android:hint="@string/hintNick"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtPasswordRegistrar" />

    <Button
        android:id="@+id/BtnRegistrarRegistrar"
        android:layout_width="@dimen/anchuraBotones"
        android:layout_height="@dimen/alturaBotones"
        android:layout_marginTop="@dimen/dim96"
        android:text="@string/Registrar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtApellido2Registrar" />

    <EditText
        android:id="@+id/txtPasswordRegistrar"
        android:layout_width="@dimen/anchuraTxt"
        android:layout_height="@dimen/alturaTxt"
        android:layout_marginTop="@dimen/dim20"
        android:autofillHints="@string/hintPassword"
        android:backgroundTint="@color/ColorPrincipal"
        android:ems="10"
        android:hint="@string/hintPassword"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtEmailRegistrar" />

    <EditText
        android:id="@+id/txtEmailRegistrar"
        android:layout_width="@dimen/anchuraTxt"
        android:layout_height="@dimen/alturaTxt"
        android:layout_marginTop="@dimen/dim96"
        android:autofillHints="@string/hintEmail"
        android:backgroundTint="@color/ColorPrincipal"
        android:ems="10"
        android:hint="@string/hintEmail"
        android:inputType="textEmailAddress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="@dimen/alturaAsterisco"
        android:layout_height="@dimen/anchuraAsterisco"
        android:layout_marginTop="@dimen/dim96"
        android:gravity="center"
        android:text="@string/fieldObligatorio"
        app:layout_constraintEnd_toStartOf="@+id/txtEmailRegistrar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>